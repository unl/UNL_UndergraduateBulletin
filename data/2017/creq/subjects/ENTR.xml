  <course>
    <title>Initiating and Managing Entrepreneurial Growth</title>
    <courseCodes>
      <courseCode type="crosslisting">
        <subject>ENTR</subject>
        <courseNumber>421</courseNumber>
      </courseCode>
      <courseCode type="home listing">
        <subject>MNGT</subject>
