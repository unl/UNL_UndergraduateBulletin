  <course>
    <title>Fundamentals of Composition</title>
    <courseCodes>
      <courseCode type="home listing">
        <subject>MUCP</subject>
        <courseNumber>382</courseNumber>
        <courseGroup>Composition (MUCP)</courseGroup>
      </courseCode>
    </courseCodes>
    <gradingType>unrestricted</gradingType>
    <dfRemoval>false</dfRemoval>
    <effectiveSemester>1078</effectiveSemester>
