  <course>
    <title>Biological and Environmental Transport Processes</title>
    <courseCodes>
      <courseCode type="crosslisting">
        <subject>AGEN</subject>
        <courseNumber>344</courseNumber>
      </courseCode>
      <courseCode type="home listing">
        <subject>BSEN</subject>
        <courseNumber>344</courseNumber>
